{
  "private": true,
  "name": "dev-site-documentation-template",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdobeDocs/dev-site-documentation-template"
  },
  "author": {
    "name": "Stephan Ringel",
    "url": "https://github.com/icaraps"
  },
  "dependencies": {
    "@adobe/gatsby-theme-aio": "^4.10.1",
    "@spectrum-css/contextualhelp": "^2.0.28",
    "@spectrum-css/link": "^4.0.40",
    "file-saver": "^2.0.5",
    "gatsby": "4.22.0",
    "http-proxy-middleware": "^2.0.6",
    "jszip": "^3.10.1",
    "jszip-utils": "^0.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "start": "gatsby build && gatsby serve",
    "start:prefix": "gatsby build --prefix-paths && gatsby serve --prefix-paths",
    "dev": "gatsby develop",
    "dev:https": "gatsby develop --https --host local.corp.adobe.com --port 8000",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:links": "remark src/pages --quiet --frail",
    "lint": "docker run --rm -e RUN_LOCAL=true --env-file '.github/super-linter.env' -v \"$PWD\":/tmp/lint github/super-linter:slim-v4.10.1"
  },
  "remarkConfig": {
    "plugins": [
      "remark-validate-links"
    ]
  },
  "packageManager": "yarn@3.2.1",
  "devDependencies": {
    "remark-cli": "^11.0.0",
    "remark-validate-links": "^12.1.0"
  }
}
